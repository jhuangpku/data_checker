python data_sampler.py -c conf/global.conf --command="{"ratio": 0.1, "input_file": "./data/a.data.utf8", "output_file": "./sample_data//a.data.utf8.sample", "decode": "gbk", "encode": "utf8", "sep": "\\n"}"
python table_checker.py -c conf/global.conf --command="{"decode": "utf8", "col_check_task": [{"fields": [{"field_no": 3, "processer": "trim", "args": ["B", "C"]}, {"field_no": 5}], "checker_args": "A", "col_checker": "float"}, {"fields": [{"field_no": 8}], "col_checker": "float"}], "sep": "\t", "col_cnt": 13, "filename": "./sample_data//a.data.utf8.sample"}"
python table_join_checker.py -c conf/global.conf --command="{"join_checker": "label_check", "files": [{"decode": "utf8", "fields": [{"field_no": 3, "processer": "trim", "args": ["B", "C"]}, {"field_no": 5}], "sep": "\t", "ratio": 0.1, "filename": "./sample_data//a.data.utf8.sample"}]}"
